salida_contable.consecutivo salida_contable,
salida.consecutivo salida_almacen,
grupo_cuentasalida, grupo_cuentaentrada,
salida.fecha_registro fecha_salida,
coalesce((NULLIF( 'Entrada No. '||entrada.consecutivo ||' del '|| entrada.fecha_registro||', Factura No.'|| entrada.numero_factura||' del '|| 
fecha_factura,'')),'Sin concepto') concepto,&quot;ESF_CODIGO_DEP&quot; as cod_dep, dep.&quot;ESF_DEP_ENCARGADA&quot; dependencia,
round(sum(total_iva_con)) as total, tipo_bienes.descripcion tipo_bien,
salida.funcionario
FROM salida
JOIN arka_inventarios.salida_contable On salida_contable.salida_general=salida.id_salida
JOIN entrada ON salida.id_entrada=entrada.id_entrada
JOIN elemento_individual ON salida.id_salida =elemento_individual.id_salida
JOIN elemento ON elemento_individual.id_elemento_gen=elemento.id_elemento
JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=elemento.nivel
JOIN grupo.grupo_descripcion ON grupo.grupo_descripcion.grupo_id=catalogo.catalogo_elemento.elemento_grupoc
JOIN arka_parametros.arka_funcionarios ON arka_parametros.arka_funcionarios.&quot;FUN_IDENTIFICACION&quot;=salida.funcionario 
JOIN arka_inventarios.tipo_bienes ON elemento.tipo_bien=id_tipo_bienes
JOIN ( SELECT DISTINCT &quot;ESF_CODIGO_DEP&quot;,&quot;ESF_DEP_ENCARGADA&quot; FROM arka_parametros.arka_dependencia) as  dep ON cast(dep.&quot;ESF_CODIGO_DEP&quot; as character varying)=salida.dependencia
WHERE 1=1
AND entrada.estado_entrada &lt;&gt;3
AND elemento.tipo_bien=salida_contable.tipo_bien
GROUP BY salida_contable,salida_almacen, fecha_salida, concepto,grupo_cuentasalida, grupo_cuentaentrada, salida.funcionario, &quot;ESF_DEP_ENCARGADA&quot;, &quot;ESF_CODIGO_DEP&quot;,tipo_bienes.descripcion
ORDER BY salida_contable.consecutivo ASC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>salida_contable</Name><TableName>salida_contable</TableName><ColumnName>consecutivo</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>salida_almacen</Name><TableName>salida</TableName><ColumnName>consecutivo</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>grupo_cuentasalida</Name><TableName></TableName><ColumnName>grupo_cuentasalida</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>grupo_cuentaentrada</N