JOIN tipo_bienes ON tipo_bienes.id_tipo_bienes=eac.tipo_bien
JOIN arka_parametros.arka_sedes as espacios ON rac.sede=cast(espacios.&quot;ESF_ID_SEDE&quot; as character varying) 
LEFT JOIN arka_parametros.arka_proveedor ON cast(proveedor as character varying)=arka_parametros.arka_proveedor.&quot;PRO_NIT&quot;
JOIN arka_parametros.arka_sedes as sedes ON sedes.&quot;ESF_COD_SEDE&quot;=espacios.&quot;ESF_COD_SEDE&quot;
