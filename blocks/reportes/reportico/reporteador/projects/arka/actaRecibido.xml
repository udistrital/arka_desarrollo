<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Acta Recibido del Bien</ReportTitle><ReportDescription>Reporte para generar Acta Recibido del Bien</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT DISTINCT id_elemento_ac ,
id_acta,
rac.fecha_registro, 
sedes.&quot;ESF_SEDE&quot; sede_ubicacion,
arka_dependencia.&quot;ESF_DEP_ENCARGADA&quot; dependencia_encargada, 
proveedor,
arka_parametros.arka_proveedor.&quot;PRO_RAZON_SOCIAL&quot; nombre_proveedor, 
fecha_revision,
revisor,
observacionesacta,
&quot;ORG_IDENTIFICACION&quot; ordenador,
&quot;ORG_NOMBRE&quot; nombre_ordenador, 
elemento_nombre nombre_nivel, 
tipo_bienes.descripcion tipo_bien,
unidad, 
cantidad,
eac.descripcion, 
valor, 
(tiva.iva*100) iva,
subtotal_sin_iva, 
total_iva, 
total_iva_con
FROM arka_inventarios.elemento_acta_recibido eac
LEFT JOIN arka_inventarios.registro_actarecibido rac ON rac.id_actarecibido=eac.id_acta
LEFT JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=eac.nivel
LEFT JOIN arka_inventarios.tipo_bienes ON tipo_bienes.id_tipo_bienes=eac.tipo_bien
LEFT JOIN arka_parametros.arka_sedes as espacios ON rac.sede=cast(espacios.&quot;ESF_ID_SEDE&quot; as character varying) 
LEFT JOIN arka_parametros.arka_proveedor ON cast(proveedor as character varying)=arka_parametros.arka_proveedor.&quot;PRO_NIT&quot;
LEFT JOIN arka_parametros.arka_sedes as sedes ON sedes.&quot;ESF_COD_SEDE&quot;=espacios.&quot;ESF_COD_SEDE&quot;
LEFT JOIN arka_parametros.arka_dependencia ON arka_dependencia.&quot;ESF_CODIGO_DEP&quot;=rac.dependencia
LEFT JOIN arka_parametros.arka_ordenadores ON ordenador_gasto=(
 select &quot;ORG_IDENTIFICACION&quot; from arka_parametros.arka_ordenadores
    limit 1
) 
LEFT JOIN arka_inventarios.aplicacion_iva tiva ON id_iva=eac.iva
WHERE 1=1 </SQLRaw></SQL><QueryColumns><QueryColumn><Name>id_elemento_ac</Name><TableName></TableName><ColumnName>DISTINCT</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>proveedor</Name><TableName></TableName><ColumnName>proveedor</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>fecha_revision</Name><TableName></TableName><ColumnName>fecha_revision</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>sede_ubicacion</Name><TableName></TableName><ColumnName>sedes.&quot;ESF_SEDE&quot;</ColumnNa