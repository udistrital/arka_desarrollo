id_hurto, 
fecha_hurto,
estado_elemento.id_elemento_ind, 
nivel,
catalogo_elemento.elemento_nombre as nivel_nombre, 
elemento_individual.placa,
marca, 
elemento_individual.serie,
total_iva,
total_iva_con, 
sedes.&quot;ESF_SEDE&quot; sede,
dependencias.&quot;ESF_DEP_ENCARGADA&quot; dependencia,
espacios.&quot;ESF_NOMBRE_ESPACIO&quot; ubicacion,
arka_parametros.arka_funcionarios.&quot;FUN_NOMBRE&quot; funnombre, 
arka_parametros.arka_funcionarios.&quot;FUN_IDENTIFICACION&quot; fun_identificacion,
salida.vigencia salida_vigencia
FROM estado_elemento
JOIN tipo_falt_sobr ON tipo_faltsobr=id_tipo_falt_sobr 
JOIN elemento_individual ON elemento_individual.id_elemento_ind=estado_elemento.id_elemento_ind 
JOIN elemento ON id_elemento_gen=id_elemento 
JOIN salida ON salida.id_entrada=elemento.id_entrada 
JOIN arka_parametros.arka_funcionarios ON elemento_individual.funcionario=arka_parametros.arka_funcionarios.&quot;FUN_IDENTIFICACION&quot;
JOIN entrada ON entrada.id_entrada=elemento.id_entrada 
JOIN arka_parametros.arka_espaciosfisicos as espacios ON espacios.&quot;ESF_ID_ESPACIO&quot;=elemento_individual.ubicacion_elemento
JOIN arka_parametros.arka_dependencia as dependencias ON dependencias.&quot;ESF_ID_ESPACIO&quot;=elemento_individual.ubicacion_elemento
JOIN arka_parametros.arka_sedes as sedes ON sedes.&quot;ESF_COD_SEDE&quot;=espacios.&quot;ESF_COD_SEDE&quot;
JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=elemento.nivel
JOIN catalogo.catalogo_lista ON catalogo.catalogo_elemento.elemento_catalogo=catalogo.catalogo_lista.lista_id
WHERE tipo_faltsobr=2 
AND elemento_individual.id_salida=salida.id_salida
AND estado_elemento.estado_registro='t' </SQLRaw></SQL><QueryColumns><QueryColumn><Name>id_elemento_ind</Name><TableName>estado_elemento</TableName><ColumnName>id_elemento_ind</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>placa</Name><TableName>elemento_individual</TableName><ColumnName>placa</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>marca</Name><TableName></TableName><ColumnName>marca</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>nivel</Name><TableName></TableName><ColumnName