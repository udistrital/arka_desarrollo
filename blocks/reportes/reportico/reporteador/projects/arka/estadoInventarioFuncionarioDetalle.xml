<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Estado Detallado Inventario Funcionario</ReportTitle><ReportDescription>Seleccione los criterios de b√∫squeda</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT DISTINCT eli.placa as placa , tb.descripcion nombre_tipo_bienes, ele.marca marca, ele.serie serie, 
CASE eli.confirmada_existencia WHEN 't' THEN 'X' ELSE ' ' END marca_existencia, 
&quot;FUN_NOMBRE&quot; nombre_funcionario, sas.&quot;ESF_SEDE&quot; sede, ad.&quot;ESF_DEP_ENCARGADA&quot; dependencia, 
CASE WHEN tfs.descripcion IS NULL THEN 'Activo' ELSE tfs.descripcion END as estado_bien, 
ele.descripcion descripcion_elemento, 
descripcion_tipoc
FROM elemento_individual eli JOIN elemento ele ON ele.id_elemento =eli .id_elemento_gen 
JOIN tipo_bienes tb ON tb.id_tipo_bienes = ele.tipo_bien
LEFT JOIN estado_elemento est ON est.id_elemento_ind = eli.id_elemento_ind 
LEFT JOIN tipo_falt_sobr tfs ON tfs.id_tipo_falt_sobr = est.tipo_faltsobr 
LEFT JOIN arka_parametros.arka_funcionarios fn ON fn.&quot;FUN_IDENTIFICACION&quot;= eli.funcionario 
JOIN arka_parametros.arka_espaciosfisicos as espacios ON espacios.&quot;ESF_ID_ESPACIO&quot;=eli.ubicacion_elemento 
JOIN arka_parametros.arka_dependencia as ad ON ad.&quot;ESF_ID_ESPACIO&quot;=eli.ubicacion_elemento 
LEFT JOIN arka_parametros.arka_sedes as sas ON sas.&quot;ESF_COD_SEDE&quot;=espacios.&quot;ESF_COD_SEDE&quot;
JOIN arka_movil.tipo_confirmacionlevantamiento ON id_tipoconfirmacion=eli.tipo_confirmada
WHERE 1=1 
AND tb.id_tipo_bienes &lt;&gt; 1 AND eli.estado_registro = 'TRUE' 
ORDER BY sede ASC </SQLRaw></SQL><QueryColumns><QueryColumn><Name>placa</Name><TableName></TableName><ColumnName>DISTINCT eli.placa as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>nombre_tipo_bienes</Name><TableName>tb</TableName><ColumnName>descripcion</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>marca</Name><TableName>ele</TableName><ColumnName>marca</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>serie</Name><TableName>ele</TableName><ColumnName>serie</ColumnName><ColumnType>ch