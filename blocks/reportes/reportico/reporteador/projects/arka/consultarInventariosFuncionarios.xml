<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Consultar Inventarios Asignados a Funcionarios</ReportTitle><ReportDescription>Seleccione los criterios de b√∫squeda</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>no</gridSearchable><gridPageable>no</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT DISTINCT 
elemento_individual.id_elemento_ind as id_elemento,
salida.consecutivo,elemento.descripcion, 
catalogo_elemento.elemento_nombre as nivel , 
marca, elemento_individual.placa,elemento_individual.serie,
coalesce(tipo_falt_sobr.descripcion,'Activo') estado_elemento,
elemento_individual.funcionario, 
&quot;FUN_NOMBRE&quot; funcionario_nombre,
sedes.&quot;ESF_SEDE&quot; sede,
dependencias.&quot;ESF_DEP_ENCARGADA&quot; nombre_dependencia,
espacios.&quot;ESF_NOMBRE_ESPACIO&quot; ubicacion,
tipo_bienes.descripcion as tipo_bien
FROM arka_inventarios.elemento_individual
LEFT JOIN arka_inventarios.elemento On elemento.id_elemento=elemento_individual.id_elemento_gen
LEFT JOIN arka_parametros.arka_espaciosfisicos as espacios ON espacios.&quot;ESF_ID_ESPACIO&quot;=elemento_individual.ubicacion_elemento
LEFT JOIN arka_parametros.arka_dependencia as dependencias ON dependencias.&quot;ESF_ID_ESPACIO&quot;=elemento_individual.ubicacion_elemento
LEFT JOIN arka_parametros.arka_sedes as sedes ON sedes.&quot;ESF_COD_SEDE&quot;=espacios.&quot;ESF_COD_SEDE&quot;
LEFT JOIN arka_parametros.arka_funcionarios ON arka_funcionarios.&quot;FUN_IDENTIFICACION&quot;=elemento_individual.funcionario
LEFT JOIN arka_inventarios.salida ON salida.id_salida=elemento_individual.id_salida
LEFT JOIN arka_inventarios.estado_elemento ON estado_elemento.id_elemento_ind=elemento_individual.id_elemento_ind
LEFT JOIN arka_inventarios.tipo_falt_sobr ON tipo_falt_sobr.id_tipo_falt_sobr=tipo_faltsobr
LEFT JOIN arka_inventarios.tipo_bienes ON tipo_bienes.id_tipo_bienes=tipo_bien
LEFT JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=elemento.nivel
WHERE 1=1
AND elemento_individual.estado_registro=TRUE  
AND tipo_bien &lt;&gt;1
ORDER By elemento_individual.id_elemento_ind</SQLRaw></SQL><QueryColumns><QueryColumn><Name>id_elemento</Name><TableName></TableName><ColumnName>DISTINCT 
