<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Informe Contabilidad Entradas General</ReportTitle><ReportDescription>Seleccione criterios del reporte</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT 
consecutivo, 
proveedor,
grupo_cuentaentrada,
ae.fecha_registro,
coalesce(NULLIF(
coalesce(tipo_contrato.descripcion||',','')||
coalesce('Contrato No.'||numero_contrato,'')||
coalesce('Factura No.'||numero_factura,'')||
coalesce(' del '||cast(fecha_factura as varchar),''),''),'Sin concepto') concepto,
round(SUM (subtotal_sin_iva)) as subtotal,
round(SUM (total_iva)) as iva,
round(SUM (total_iva_con)) as total
FROM arka_inventarios.entrada ae
LEFT JOIN arka_inventarios.elemento aele ON ae.id_entrada=aele.id_entrada
LEFT JOIN arka_inventarios.estado_entrada on estado_entrada.id_estado=estado_entrada
LEFT JOIN arka_inventarios.tipo_contrato ON tipo_contrato.id_tipo=tipo_contrato
LEFT JOIN arka_inventarios.clase_entrada ON clase_entrada.id_clase=ae.clase_entrada
LEFT JOIN arka_parametros.arka_proveedor ON arka_parametros.arka_proveedor.&quot;PRO_NIT&quot;=cast(proveedor as character varying)
LEFT JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=aele.nivel
LEFT JOIN grupo.grupo_descripcion ON grupo.grupo_descripcion.grupo_id=catalogo.catalogo_elemento.elemento_grupoc
LEFT JOIN arka_inventarios.aplicacion_iva ON id_iva=aele.iva
WHERE 1=1
AND ae.estado_entrada &lt;&gt; 3
GROUP BY consecutivo, 
proveedor,
grupo_cuentaentrada,
ae.fecha_registro,concepto
ORDER BY consecutivo</SQLRaw></SQL><QueryColumns><QueryColumn><Name>consecutivo</Name><TableName></TableName><ColumnName>consecutivo</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>proveedor</Name><TableName></TableName><ColumnName>proveedor</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>grupo_cuentaentrada</Name><TableName></TableName><ColumnName>grupo_cuentaentrada</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>fecha_registro</Name><TableName>ae</TableName><ColumnName>fecha_registro