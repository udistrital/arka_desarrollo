<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Observaciones Inventario Funcionario</ReportTitle><ReportDescription>Seleccione los criterios de búsqueda</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT DISTINCT 
ROW_NUMBER() OVER(ORDER BY sa.&quot;ESF_SEDE&quot; ASC,&quot;ESF_DEP_ENCARGADA&quot;ASC) AS consecutivo,
dl.id_periodolevantamiento id_periodo,
dl.fecha_registro fecha_observacion,
fecha_inicio, 
fecha_final,
aei.placa,
descripcion,
sa.&quot;ESF_SEDE&quot; sede,
ad.&quot;ESF_DEP_ENCARGADA&quot; dependencia, 
&quot;FUN_IDENTIFICACION&quot; funcionario,&quot;FUN_NOMBRE&quot; nombre_funcionario,
coalesce(observacion,'Sin observación') observacion,
coalesce(CASE WHEN creador_observacion=0 THEN 'Funcionario' WHEN creador_observacion=1 THEN 'Almacen' END, 'Sin informador') informador, 
CASE WHEN tipo_confirmada=1 THEN 'Aprobado' WHEN tipo_confirmada=2 THEN 'No Aprobado' WHEN tipo_confirmada=0 THEN 'Sin revisar' END confirmacion_2
FROM arka_movil.periodo_levantamiento pl,arka_inventarios.elemento_individual aei
JOIN arka_inventarios.elemento ele ON ele.id_elemento=aei.id_elemento_gen
LEFT JOIN arka_movil.detalle_levantamiento dl ON dl.id_elemento_individual=aei.id_elemento_ind
JOIN arka_parametros.arka_funcionarios af ON af.&quot;FUN_IDENTIFICACION&quot;=aei.funcionario
left JOIN arka_parametros.arka_espaciosfisicos ef ON  ef.&quot;ESF_ID_ESPACIO&quot;=aei.ubicacion_elemento
left JOIN arka_parametros.arka_dependencia as ad ON ad.&quot;ESF_ID_ESPACIO&quot;=ef.&quot;ESF_ID_ESPACIO&quot;
left JOIN arka_parametros.arka_sedes sa ON sa.&quot;ESF_COD_SEDE&quot;=ef.&quot;ESF_COD_SEDE&quot;
WHERE 1=1
AND aei.placa!=''
AND dl.id_elemento_individual=aei.id_elemento_ind
AND dl.id_periodolevantamiento=pl.id_periodolevantamiento
GROUP BY &quot;ESF_SEDE&quot;,&quot;ESF_DEP_ENCARGADA&quot;, &quot;FUN_IDENTIFICACION&quot;,&quot;FUN_NOMBRE&quot;,fecha_inicio, fecha_final, aei.placa, tipo_confirmada, descripcion, observacion, creador_observacion,dl.fecha_registro,dl.id_periodolevantamiento
ORDER BY consecutivo,sede ASC, dl.id_periodolevantamiento ASC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>consecutivo</Name><TableName></TableName><ColumnName>DISTINCT 
