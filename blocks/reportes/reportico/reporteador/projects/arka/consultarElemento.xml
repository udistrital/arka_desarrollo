id_elemento, 
salida.fecha_registro as fecha_salida,
placa,
grupo_cuentasalida cuenta_salida,  
grupo.catalogo_elemento.elemento_nombre nombre_cuenta,  
catalogo.catalogo_elemento.elemento_nombre nombre_nivel,
elemento.descripcion descripcion_elemento, 
cantidad,
total_iva_con,
tipo_bienes.descripcion tipo_bien,
&quot;FUN_NOMBRE&quot;as funcionario,
sedes.&quot;ESF_SEDE&quot; sede_ubicacion,
dependencias.&quot;ESF_DEP_ENCARGADA&quot; dependencia_encargada, 
espacios.&quot;ESF_NOMBRE_ESPACIO&quot; ubicacion,upper(marca) marca, upper(elemento.serie) serie
FROM elemento
JOIN entrada ON entrada.id_entrada=elemento.id_entrada
JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=elemento.nivel
JOIN tipo_bienes ON tipo_bienes.id_tipo_bienes=tipo_bien
JOIN elemento_individual ON id_elemento_gen=id_elemento
JOIN salida ON salida.id_salida=elemento_individual.id_salida
JOIN arka_parametros.arka_funcionarios ON arka_parametros.arka_funcionarios.&quot;FUN_IDENTIFICACION&quot;=elemento_individual.funcionario
JOIN arka_parametros.arka_espaciosfisicos as espacios ON espacios.&quot;ESF_ID_ESPACIO&quot;=elemento_individual.ubicacion_elemento
JOIN arka_parametros.arka_dependencia as dependencias ON dependencias.&quot;ESF_ID_ESPACIO&quot;=elemento_individual.ubicacion_elemento
JOIN arka_parametros.arka_sedes as sedes ON sedes.&quot;ESF_COD_SEDE&quot;=espacios.&quot;ESF_COD_SEDE&quot;
JOIN grupo.catalogo_elemento ON cast(grupo.catalogo_elemento.elemento_id as character varying)=catalogo.catalogo_elemento.elemento_grupoc
JOIN grupo.grupo_descripcion ON grupo.grupo_descripcion.grupo_id=cast(grupo.catalogo_elemento.elemento_id as character varying)
WHERE elemento_individual.estado_registro=TRUE
AND estado_entrada &lt;&gt; 3
ORDER BY sede_ubicacion,placa</SQLRaw></SQL><QueryColumns><QueryColumn><Name>id_elemento</Name><TableName></TableName><ColumnName>id_elemento</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>fecha_salida</Name><TableName></TableName><ColumnName>salida.fecha_registro as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>cantidad</Name><TableName></TableName><ColumnName>cantidad</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>cuenta_salida</Name><TableName></TableName><ColumnName>grupo_cuenta