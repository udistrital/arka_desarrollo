<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Informe Contabilidad de Entradas</ReportTitle><ReportDescription>Seleccione los criterios del informe</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>no</gridSearchable><gridPageable>no</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT 
consecutivo, 
ae.fecha_registro,
proveedor,
&quot;PRO_RAZON_SOCIAL&quot; razon_social,valor,
aplicacion_iva.iva,
total_iva,
cantidad,
grupo_cuentaentrada,
coalesce(NULLIF(
coalesce(tipo_contrato.descripcion||',','')||
coalesce('Contrato No.'||numero_contrato,'')||
coalesce('Factura No.'||numero_factura,'')||
coalesce(' del '||cast(fecha_factura as varchar),''),''),'Sin concepto') concepto,
subtotal_sin_iva,
total_iva_con
FROM arka_inventarios.entrada ae
JOIN arka_inventarios.elemento aele ON ae.id_entrada=aele.id_entrada
JOIN arka_inventarios.estado_entrada on estado_entrada.id_estado=estado_entrada
LEFT JOIN arka_inventarios.tipo_contrato ON tipo_contrato.id_tipo=tipo_contrato
JOIN arka_inventarios.clase_entrada ON clase_entrada.id_clase=ae.clase_entrada
LEFT JOIN arka_parametros.arka_proveedor ON arka_parametros.arka_proveedor.&quot;PRO_NIT&quot;=cast(proveedor as character varying)
JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=aele.nivel
JOIN grupo.grupo_descripcion ON grupo.grupo_descripcion.grupo_id=cast(nivel as character varying)
JOIN arka_inventarios.aplicacion_iva ON id_iva=aele.iva
ORDER BY consecutivo ASC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>consecutivo</Name><TableName></TableName><ColumnName>consecutivo</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Consecutivo Entrada</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>fecha_registro</Name><TableName>ae</TableName><ColumnName>fecha_registro</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>grupo_cuentaentrada</Name><TableName></TableName><ColumnName>grupo_cuentaentrada</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>razon_social</Name><TableName></Table