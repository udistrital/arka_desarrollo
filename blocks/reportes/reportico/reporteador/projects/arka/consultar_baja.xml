<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Consultar Bajas Solicitadas</ReportTitle><ReportDescription>Seleccione los criterios de b√∫squeda</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin>1cm</BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>6</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>no</gridSearchable><gridPageable>no</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT nivel,
catalogo.catalogo_elemento.elemento_nombre as nivel_nombre, 
sedes.&quot;ESF_SEDE&quot; sede,
dependencias.&quot;ESF_DEP_ENCARGADA&quot; dependencia,
espacios.&quot;ESF_NOMBRE_ESPACIO&quot; ubicacion,
id_baja, 
baja_elemento.id_elemento_ind, 
elemento_individual.placa, 
elemento.descripcion nombre,
marca,
elemento_individual.serie,
total_iva,
total_iva_con,
baja_elemento.fecha_registro,
tipo_baja.descripcion  causa_baja,
(CASE WHEN estado_aprobacion='t' THEN 'Baja Aprobada'
ELSE 'Baja por Aprobar'
END) estado_aprobacion,
(elemento.total_iva_con - coalesce(elemento.ajuste,0) + ajuste_inflacionario) as total_valor_ajustado,
ValorLibroPrueba(salida.fecha_registro,grupo_vidautil,elemento.total_iva_con - coalesce(elemento.ajuste,0),0,ajuste_inflacionario,2) total_depreciacion,
ValorLibroPrueba(salida.fecha_registro,grupo_vidautil,elemento.total_iva_con - coalesce(elemento.ajuste,0),0,ajuste_inflacionario,3) valor_libros,
baja_elemento.usuario 
FROM baja_elemento
LEFT JOIN elemento_individual ON elemento_individual.id_elemento_ind=baja_elemento.id_elemento_ind
LEFT JOIN elemento ON id_elemento_gen=id_elemento
LEFT JOIN salida ON salida.id_salida=elemento_individual.id_salida
LEFT JOIN entrada ON entrada.id_entrada=elemento.id_entrada
LEFT JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=elemento.nivel
LEFT JOIN catalogo.catalogo_lista ON catalogo.catalogo_elemento.elemento_catalogo=catalogo.catalogo_lista.lista_id
LEFT JOIN arka_parametros.arka_espaciosfisicos as espacios ON espacios.&quot;ESF_ID_ESPACIO&quot;=elemento_individual.ubicacion_elemento
LEFT JOIN arka_parametros.arka_dependencia as dependencias ON dependencias.&quot;ESF_ID_ESPACIO&quot;=elemento_individual.ubicacion_elemento
LEFT JOIN arka_parametros.arka_sedes as sedes ON sedes.&quot;ESF_COD_SEDE&quot;=espacios.&quot;ESF_COD_SEDE&quot;
LEFT JOIN grupo.catalogo_elemento ON cast(grupo.catalogo_elemento.elemento_id as character varying)=catalogo.catalogo_elemento.elemento_grupoc 
LEFT JOIN grupo.grupo_descripcion ON grupo.grupo_descripcion.grupo_id=cast(grupo.catalogo_elemento.elemento_id as character varying)   
LEFT JOIN tipo_baja ON tipo_baja.id_tipo_baja=tipo_baja
WHERE baja_elemento.estado_registro='t'</SQLRaw></SQL><QueryColumns><QueryColumn><Name>nivel</Name><TableName></TableName><ColumnName>nivel</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>nivel_nombre</Name><TableName></TableName><ColumnName>catalogo.catalogo_elemento.elemento_nombre as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>sede</Name><TableName></TableName><ColumnName>sedes.&quot;ESF_SEDE&quot;</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>dependencia</Name><TableName></Tab