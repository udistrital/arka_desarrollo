<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Consultar Inventarios Asignados a Contratistas</ReportTitle><ReportDescription>Seleccione los criterios correspondientes para consultar los elementos asignados a uno o varios contratistas</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>no</gridSearchable><gridPageable>no</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT elemento_individual.id_elemento_ind,
elemento.descripcion,elemento_nombre nivel, marca, 
elemento_individual.placa,elemento_individual.serie, 
arka_parametros.arka_contratistas.&quot;CON_NOMBRE&quot; as contratista_nombre, 
arka_parametros.arka_contratistas.&quot;CON_IDENTIFICACION&quot; as contratista_identificacion,
sedes.&quot;ESF_SEDE&quot; sede,
dependencias.&quot;ESF_DEP_ENCARGADA&quot; nombre_dependencia,
espacios.&quot;ESF_NOMBRE_ESPACIO&quot; ubicacion,
coalesce(tipo_falt_sobr.descripcion,'Activo') as estado_elemento
FROM arka_parametros.arka_contratistas,asignar_elementos, elemento
JOIN elemento_individual ON elemento.id_elemento=elemento_individual.id_elemento_gen 
JOIN salida on elemento_individual.id_salida=salida.id_salida
JOIN arka_parametros.arka_espaciosfisicos as espacios ON espacios.&quot;ESF_ID_ESPACIO&quot;=elemento_individual.ubicacion_elemento
JOIN arka_parametros.arka_dependencia as dependencias ON dependencias.&quot;ESF_ID_ESPACIO&quot;=elemento_individual.ubicacion_elemento
JOIN arka_parametros.arka_sedes as sedes ON sedes.&quot;ESF_COD_SEDE&quot;=espacios.&quot;ESF_COD_SEDE&quot;
JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=elemento.nivel
LEFT JOIN estado_elemento ON estado_elemento.id_elemento_ind=elemento_individual.id_elemento_ind
LEFT JOIN tipo_falt_sobr ON tipo_falt_sobr.id_tipo_falt_sobr=tipo_faltsobr
WHERE elemento_individual.estado_registro=TRUE  
AND asignar_elementos.contratista=arka_parametros.arka_contratistas.&quot;CON_IDENTIFICACION&quot;
AND elemento_individual.id_elemento_ind=asignar_elementos.id_elemento  
AND elemento_individual.estado_asignacion=TRUE  
AND asignar_elementos.estado=1
AND salida.id_entrada=elemento.id_entrada
AND salida.id_salida=elemento_individual.id_salida</SQLRaw></SQL><QueryColumns><QueryColumn><Name>descripcion</Name><TableName>elemento</TableName><ColumnName>descripcion</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>marca</Name><TableName></TableName><ColumnName>marca</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>nivel</Name><TableName></TableName><ColumnName>elemento_nombre</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>placa</Name><TableName>elemento_individual</TableName><Colum