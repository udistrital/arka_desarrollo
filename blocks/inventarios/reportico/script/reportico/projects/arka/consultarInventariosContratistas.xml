<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Consultar Inventarios Asignados a Contratistas</ReportTitle><ReportDescription>Seleccione los criterios correspondientes para consultar los elementos asignados a uno o varios contratistas</ReportDescription><PageSize>US-Letter</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin>1cm</BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>no</gridSearchable><gridPageable>no</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>none</DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT 
nivel,  
unidad, 
cantidad,  
marca,  
serie, 
valor, 
subtotal_sin_iva,  
total_iva, 
total_iva_con, 
contratista_servicios.identificacion, 
elemento.id_elemento, supervisor
FROM elemento, asignar_elementos,contratista_servicios, salida
JOIN funcionario ON funcionario.id_funcionario = salida.funcionario
WHERE cast(contratista_servicios.identificacion as char)=cast(asignar_elementos.contratista as char)
AND elemento.estado=TRUE
AND elemento.id_elemento=asignar_elementos.id_elemento 
AND elemento.estado_asignacion=TRUE 
