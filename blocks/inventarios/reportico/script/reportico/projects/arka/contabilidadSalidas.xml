  cod_sede,sum(total_iva_con) column11,tipo_bienes.descripcion,identificacion, funcionario.nombre       
  FROM entrada
  JOIN clase_entrada ON clase_entrada.id_clase=entrada.clase_entrada
  JOIN elemento ON elemento.id_entrada=entrada.id_entrada
  JOIN salida ON salida.id_entrada=entrada.id_entrada
  JOIN tipo_bienes ON elemento.tipo_bien=id_tipo_bienes
  JOIN funcionario ON funcionario=id_funcionario
  JOIN dependencia ON funcionario.dependencia=dependencia.id_dependencia
  GROUP BY  salida.id_salida,entrada.id_entrada, entrada.fecha_registro, vigencia, clase_entrada.descripcion, clase_entrada, 
       numero_factura, fecha_factura, tipo_bienes.descripcion, identificacion, funcionario.nombre, dependencia.nombre, cod_sede
