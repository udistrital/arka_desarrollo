FROM (
SELECT placa,sede cod_sede, &quot;ESF_SEDE&quot; sede,dependencia dependencia_cod, &quot;ESF_NOMBRE_ESPACIO&quot; dependencia, funcionario, &quot;FUN_NOMBRE&quot; funcionario_nombre,coalesce(tipo_falt_sobr.descripcion,  'Activo') estado_elemento
FROM arka_movil.registro_elementos_funcionario
JOIN elemento_individual ON elemento_individual.id_elemento_ind=arka_movil.registro_elementos_funcionario.id_elemento
LEFT JOIN estado_elemento ON arka_movil.registro_elementos_funcionario.id_elemento=estado_elemento.id_elemento_ind
LEFT JOIN tipo_falt_sobr ON tipo_falt_sobr.id_tipo_falt_sobr=tipo_faltsobr
JOIN arka_parametros.arka_funcionarios pasado ON pasado.&quot;FUN_IDENTIFICACION&quot;=arka_movil.registro_elementos_funcionario.funcionario
JOIN arka_parametros.arka_espaciosfisicos ON arka_parametros.arka_espaciosfisicos.&quot;ESF_ID_ESPACIO&quot;=arka_movil.registro_elementos_funcionario.dependencia
JOIN arka_parametros.arka_sedes ON cast(arka_parametros.arka_sedes.&quot;ESF_ID_SEDE&quot; as character varying)=sede
UNION
SELECT placa,sede cod_sede, &quot;ESF_SEDE&quot; ,dependencia dependencia_cod, &quot;ESF_NOMBRE_ESPACIO&quot; dependencia,funcionario,&quot;FUN_NOMBRE&quot; funcionario_nombre,coalesce(tipo_falt_sobr.descripcion, 'Activo') estado_elemento
FROM salida
JOIN elemento_individual ON elemento_individual.id_salida=salida.id_salida
LEFT JOIN estado_elemento ON elemento_individual.id_elemento_ind=estado_elemento.id_elemento_ind
JOIN tipo_falt_sobr ON tipo_falt_sobr.id_tipo_falt_sobr=tipo_faltsobr
JOIN arka_parametros.arka_funcionarios pasado ON pasado.&quot;FUN_IDENTIFICACION&quot;=salida.funcionario
JOIN arka_parametros.arka_espaciosfisicos ON arka_parametros.arka_espaciosfisicos.&quot;ESF_ID_ESPACIO&quot;=salida.dependencia
JOIN arka_parametros.arka_sedes ON cast(arka_parametros.arka_sedes.&quot;ESF_ID_SEDE&quot; as character varying)=sede
) as elementos_asignados
