entrada.consecutivo entrada_consecutivo,
clase_entrada.descripcion clase_entrada,
salida.consecutivo salida_consecutivo,
salida.fecha_registro, 
sedes.&quot;ESF_SEDE&quot; sede,
dependencias.&quot;ESF_DEP_ENCARGADA&quot; dependencia, 
espacios.&quot;ESF_NOMBRE_ESPACIO&quot; ubicacion,
arka_parametros.arka_funcionarios.&quot;FUN_NOMBRE&quot; nombrefun, 
arka_parametros.arka_funcionarios.&quot;FUN_IDENTIFICACION&quot; idfun,
elemento_nombre nivel,
elemento_individual.placa,
elemento_individual.serie,
salida.vigencia,
cantidad, valor,iva, total_iva_con,
elemento.descripcion, tipo_bienes.descripcion tipo_bien
FROM salida 
JOIN entrada ON salida.id_entrada=entrada.id_entrada 
JOIN elemento_individual ON elemento_individual.id_salida=salida.id_salida
JOIN arka_parametros.arka_funcionarios ON elemento_individual.funcionario=arka_parametros.arka_funcionarios.&quot;FUN_IDENTIFICACION&quot;
JOIN arka_parametros.arka_espaciosfisicos as espacios ON espacios.&quot;ESF_ID_ESPACIO&quot;=elemento_individual.ubicacion_elemento
JOIN arka_parametros.arka_dependencia as dependencias ON dependencias.&quot;ESF_ID_ESPACIO&quot;=elemento_individual.ubicacion_elemento
JOIN arka_parametros.arka_sedes as sedes ON sedes.&quot;ESF_COD_SEDE&quot;=espacios.&quot;ESF_COD_SEDE&quot;
JOIN elemento on elemento_individual.id_elemento_gen=elemento.id_elemento
JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=elemento.nivel
JOIN clase_entrada ON clase_entrada.id_clase=entrada.clase_entrada
JOIN tipo_bienes ON id_tipo_bienes=elemento.tipo_bien
ORDER BY salida.id_salida ASC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>entrada_consecutivo</Name><TableName>entrada</TableName><ColumnName>consecutivo</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>clase_entrada</Name><TableName>clase_entrada</TableName><ColumnName>descripcion</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>salida_consecutivo</Name><TableName>salida</TableName><ColumnName>consecutivo</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>fecha_registro</Name><TableName>salida