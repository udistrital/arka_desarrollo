placa,
sedes.&quot;ESF_SEDE&quot; sede,
dependencias.&quot;ESF_DEP_ENCARGADA&quot; dependencia,
espacios.&quot;ESF_NOMBRE_ESPACIO&quot; ubicacion,
pasado.&quot;FUN_NOMBRE&quot; as funcionario_anterior,
actual.&quot;FUN_NOMBRE&quot; as funcionario_siguiente,
marca, 
elemento_individual.serie,
historial_elemento_individual.observaciones observaciones_traslados,
elemento_nombre as nombre_nivel,
salida.vigencia vigenciasalida,
elemento.descripcion as descripcion_elemento
FROM elemento_individual
JOIN elemento ON id_elemento_gen = elemento.id_elemento
JOIN historial_elemento_individual ON historial_elemento_individual.elemento_individual = elemento_individual.id_elemento_ind 
JOIN salida ON salida.id_salida = elemento_individual.id_salida
JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=nivel
JOIN arka_parametros.arka_funcionarios actual ON actual.&quot;FUN_IDENTIFICACION&quot;=elemento_individual.funcionario
JOIN arka_parametros.arka_funcionarios pasado ON cast(pasado.&quot;FUN_IDENTIFICACION&quot; as character varying)=historial_elemento_individual.descripcion_funcionario
JOIN arka_parametros.arka_espaciosfisicos as espacios ON espacios.&quot;ESF_ID_ESPACIO&quot;=elemento_individual.ubicacion_elemento
JOIN arka_parametros.arka_dependencia as dependencias ON dependencias.&quot;ESF_ID_ESPACIO&quot;=elemento_individual.ubicacion_elemento
JOIN arka_parametros.arka_sedes as sedes ON sedes.&quot;ESF_COD_SEDE&quot;=espacios.&quot;ESF_COD_SEDE&quot;
JOIN catalogo.catalogo_lista ON catalogo.catalogo_elemento.elemento_catalogo=catalogo.catalogo_lista.lista_id
ORDER BY historial_elemento_individual.fecha_registro ASC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>marca</Name><TableName></TableName><ColumnName>marca</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>sede</Name><TableName></TableName><ColumnName>sedes.&quot;ESF_SEDE&quot;</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>serie</Name><TableName>elemento_individual</TableName><ColumnName>serie</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>dependencia</Name><TableName></TableName><ColumnNa