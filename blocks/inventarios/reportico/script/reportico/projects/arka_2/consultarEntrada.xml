<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Reporte Entrada de Almacén</ReportTitle><ReportDescription>Informe que muestra información de la entrada seleccionada</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin>1cm</BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><bodyDisplay>show</bodyDisplay><graphDisplay>show</graphDisplay><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>yes</gridSearchable><gridPageable>no</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>none</DatabaseType><DatabaseName></DatabaseName><HostName>localhost</HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT entrada.consecutivo,entrada.id_entrada, entrada.fecha_registro fecha_entrada,clase_entrada.descripcion clase_entrada, 
 &quot;PRO_NIT&quot; proveedornit , &quot;PRO_RAZON_SOCIAL&quot; proveedor ,
(tipo_contrato.descripcion ||', '||numero_contrato ||', '||fecha_contrato ||', '|| &quot;PRO_NIT&quot;  ||', '|| &quot;PRO_RAZON_SOCIAL&quot;  ||', '||numero_factura ||', '|| fecha_factura ) concepto 
,observaciones, 
placa,elemento.descripcion descripcion_elemento,
catalogo_elemento.elemento_nombre as nivel_inventario, elemento.unidad, elemento.cantidad, elemento.marca,
elemento.serie, elemento.valor, elemento.subtotal_sin_iva, elemento.total_iva, elemento.total_iva_con,
clase_entrada.descripcion clase_entrada
FROM entrada
JOIN elemento ON elemento.id_entrada=entrada.id_entrada
JOIN elemento_individual ON elemento_individual.id_elemento_gen=elemento.id_elemento
INNER JOIN arka_parametros.arka_proveedor ON arka_parametros.arka_proveedor.&quot;PRO_NIT&quot;= CAST(entrada.proveedor AS CHARACTER VARYING)
JOIN catalogo.catalogo_elemento ON catalogo.catalogo_elemento.elemento_id=elemento.nivel
JOIN catalogo.catalogo_lista ON catalogo.catalogo_elemento.elemento_catalogo=catalogo.catalogo_lista.lista_id
JOIN clase_entrada ON clase_entrada.id_clase=entrada.clase_entrada 
JOIN tipo_contrato ON tipo_contrato.id_tipo=entrada.tipo_contrato
WHERE elemento.id_entrada=entrada.id_entrada AND entrada.estado_registro='T'
AND catalogo.catalogo_lista.lista_activo=1</SQLRaw></SQL><QueryColumns><QueryColumn><Name>total_subtotal</Name><TableName></TableName><ColumnName></ColumnName><ColumnType></ColumnType><ColumnLength></ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>fecha_entrada</Name><TableName>entrada</TableName><ColumnName>fecha_registro</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>proveedornit</Name><TableName></TableName><ColumnName>&quot;PRO_NIT&quot;</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label></group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label></group_trailer_label></Format></QueryColumn><QueryColumn><Name>proveedor</Name><TableName></TableName><ColumnName>&quot;PRO_